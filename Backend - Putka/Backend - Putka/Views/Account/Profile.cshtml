                                <div class="dashboard-order">
                                    <div class="title">
                                        <h2>My Orders History</h2>
                                    </div>

                                    <div class="order-contain">

                                        @foreach (var order in Model.Orders)
                                        {
                                            count++;

                                            <div class="order-box dashboard-bg-box">
                                                <div class="order-container">
                                                    <div class="order-icon">
                                                        <i data-feather="box"></i>
                                                    </div>

                                                    <div class="order-detail d-flex flex-row">
                                                        <h4>Delivered <span class="@(order.Status == OrderStatus.Pending ? "" : (order.Status == OrderStatus.Accepted ? "success-bg" : "failed-bg"))">@order.Status.ToString()</span></h4>
                                                        <ul class="vertical-list" style="list-style-type: none; padding: 0; display: flex; flex-direction: column; margin-left: 300px">
                                                            <li> <h4 class="text-content">No: @count</h4> </li>
                                                            <li> <h4 class="text-content">Order(#): @((User.Identity.Name.Substring(0, 1) + order.UserName.Substring(0, 1)).ToUpper())-@(order.CreatedDate.Minute)</h4> </li>
                                                            <li> <h4 class="text-content">Date: @order.CreatedDate.ToString("dd-MM-yyyy")</h4> </li>
                                                            <li> <h4 class="text-content" style="color: #0DA487">Total: @order.OrderItems.Sum(x=>x.Count * ((x.Product.DiscountPercent > 0) ? (x.Product.SalePrice * (100 - x.Product.DiscountPercent) / 100) : x.Product.SalePrice)).ToString("0.00")$</h4> </li>
                                                        </ul>
                                                    </div>
                                                </div>

                                                @foreach (var item in order.OrderItems)
                                                {
                                                    <div class="product-order-detail">
                                                        <a href="product-left-thumbnail.html" class="order-image">
                                                            <img src="~/uploads/products/@item.Product.ProductImages.FirstOrDefault(x=>x.IsMain == true)?.Url"
