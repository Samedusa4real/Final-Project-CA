                                                            </a>
                                                            <ul class="product-size">
                                                                <li>
                                                                    <div class="size-box">
                                                                        <h6 class="text-content">Price : </h6>
                                                                        <h5>$@(((item.Product.DiscountPercent > 0) ? (item.Product.SalePrice * (100 - item.Product.DiscountPercent) / 100) : item.Product.SalePrice).ToString("0.00"))</h5>
                                                                    </div>
                                                                </li>

                                                                <li>
                                                                    <div class="size-box">
                                                                        <h6 class="text-content">Quantity : </h6>
                                                                        <h5>@item.Count</h5>
                                                                    </div>
                                                                </li>
                                                            </ul>
                                                        </div>
                                                    </div>
                                                }
                                            </div>
                                        }
